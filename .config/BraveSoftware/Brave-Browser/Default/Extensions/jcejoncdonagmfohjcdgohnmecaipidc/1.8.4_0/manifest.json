{
   "background": {
      "scripts": [ "/polyfill.js", "/background.js" ]
   },
   "browser_action": {
      "browser_style": false,
      "default_icon": "/icons/icon.png",
      "default_popup": "/popup.html"
   },
   "description": "Monitors source files on a page. Reloads the page or just the changed stylesheet when source files are updated.",
   "homepage_url": "https://github.com/blaise-io/live-reload#readme",
   "icons": {
      "128": "/icons/icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Live Reload",
   "options_ui": {
      "page": "/options.html"
   },
   "permissions": [ "\u003Call_urls>", "storage", "tabs", "webNavigation", "webRequest" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8.4",
   "web_accessible_resources": [ "/polyfill.js" ]
}
